{
  "name": "app-engine",
  "version": "1.0.0",
  "description": "Backend application instance",
  "main": "index.js",
  "scripts": {
    "test": "mocha dist/identity-authority/users/tests/services/access-user.test.js",
    "test2": "tsc && mocha dist/**/*.test.js",
    "container:build": "node bin/container/build.js",
    "start:development": "node bin/container/build.js && tsc && node dist/core/bootstrap.js --env=development --port=5458",
    "migrate:mysql": "tsc && node bin/migrations/mysql",
    "migrate:development:start": "node bin/migrations/umzug --env=development --action=up",
    "migrate:development": "node bin/migrations/umzug --env=development --action=next",
    "migrate:development:rollback": "node bin/migrations/umzug --env=development --action=down"
  },
